{
  "logging": {
    "level": "TRACE"
  },
  "worker": {
    "name": "Test",
    "description": "",
    "projects": {
      "trust_layer_metadata": true,
      "disable_getprint": true,
      "force_readonly_layers": true,
      "disable_advertised_urls": false,
      "search_paths": {
        "/tests": "file:///opt/qjazz/data/samples",
        "/france": "file:///opt/qjazz/data/france_parts",
        "/montpellier": "file:///opt/qjazz/data/montpellier"
      },
      "allow_direct_path_resolution": true
    },
    "max_projects": 1,
    "load_project_on_request": true,
    "reload_outdated_project_on_request": false,
    "plugins": {
      "paths": [
        "/home/tests/plugins"
      ]
    },
    "max_chunk_size": 1048576,
    "interfaces": [
      {
        "listen": [
          "[::]",
          23456
        ],
        "ssl": {
          "cafile": null,
          "certfile": null,
          "keyfile": null
        }
      }
    ],
    "max_waiting_requests": 20,
    "process_timeout": 20,
    "shutdown_grace_period": 5,
    "max_processes_failure_pressure": 0.0
  }
}
